        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "His Rani, Indra Mukhi, was wondrously pretty.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਦੀ ਇੰਦ੍ਰ ਮੁਖੀ ਨਾ ਦੀ ਬਹੁਤ ਸੁੰਦਰ ਰਾਣੀ ਸੀ।",
              "additional_information": {}
            }
          }
        }
