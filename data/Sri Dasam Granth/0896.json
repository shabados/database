        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She was as beautiful as Sachi (the consort of god Indra),(2)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਸ ਨੂੰ ਸਚੀ ਵਰਗਾ ਸਮਝ ਕੇ ਆਪ ਇੰਦਰ ਹੈਰਾਨ ਹੋ ਜਾਦਾ ਸੀ ॥੨॥",
              "additional_information": {}
            }
          }
        }
      }
