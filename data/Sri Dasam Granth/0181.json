          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਹ ਰੁਦਰ ਦੇ ਸ਼ਰੀਰ ਵਿਚ ਲਗੇ ਅਤੇ ਜ਼ਖ਼ਮ ਕਰ ਕੇ",
              "additional_information": {}
            }
