          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਉਸ ਨੇ) ਸ਼ਿਵ ਦੀ ਸਵਾਰੀ ਨੰਦੀ ਬਲਦ ਨੂੰ ਅਣਗਿਣਤ ਤੀਰ ਮਾਰੇ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "Z37W",
        "source_page": 181,
        "source_line": null,
        "gurmukhi": "byiD AMg kh pwr pDwry ]33]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He discharged many arrows on Nandi, which penetrated through his limbs.33.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜੋ ਉਸ ਦੇ ਸ਼ਰੀਰ ਵਿਚੋਂ ਪਾਰ ਹੋ ਗਏ ॥੩੩॥",
              "additional_information": {}
            }
          }
        }
      },
      {
