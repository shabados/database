        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When Rudra shot his arrows, the army of demons sped away.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦੋਂ ਦੈਂਤਾ ਦੀ ਸੈਨਾ ਭਜ ਗਈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
