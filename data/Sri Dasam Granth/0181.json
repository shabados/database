        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then Andhakasura came in front of Shiva, a dreadful war ensured.35.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ (ਦੋਹਾ ਨੇ) ਰਣ-ਭੂਮੀ ਵਿਚ ਦੁਅੰਦ-ਯੁੱਧ ਮਚਾਇਆ ॥੩੫॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
