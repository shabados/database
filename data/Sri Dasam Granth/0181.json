        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "At that time, the mighty Andhakasura, alongwith the army of demons sped towards the fornt.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਬਲਵਾਨ ਅੰਧਕ ਨੇ ਧਾਵਾ ਬੋਲਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "JS3G",
        "source_page": 181,
        "source_line": null,
