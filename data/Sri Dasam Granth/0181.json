        "source_page": 181,
        "source_line": null,
        "gurmukhi": "jb hI bwx lgy bwhx qin ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦੋਂ ਨੰਦੀ ਬਲਦ ਦੇ ਸ਼ਰੀਰ ਵਿਚ ਤੀਰ ਲਗੇ,",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "JNLF",
        "source_page": 181,
        "source_line": null,
        "gurmukhi": "ros jigXo qb hI isv ky min ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When the god Shiva saw the infliction of the arrows on his vehicle, then he became violently furious.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦੋਂ ਸ਼ਿਵ ਦੇ ਮਨ ਵਿਚ ਕ੍ਰੋਧ ਭੜਕ ਉਠਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "VCCV",
