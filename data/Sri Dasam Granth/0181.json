          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਭਥੇ ਵਿਚੋਂ ਦੋ ਤੀਰ ਕਢ ਕੇ ਦੁਸ਼ਟ ਨੂੰ ਮਾਰੇ",
              "additional_information": {}
            }
          }
        }
