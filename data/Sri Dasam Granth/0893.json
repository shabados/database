        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "All those who were sleeping, awake, standing or sitting got stuck with the ground.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸੌਂਦੇ, ਜਾਗਦੇ, ਉਠਦੇ, ਬੈਠਦੇ ਸਾਰੇ ਹੀ ਛਿਣ ਭਰ ਵਿਚ ਚਿਮਟ ਗਏ।",
              "additional_information": {}
            }
          }
        }
