        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Husband was stuck while tying his lion-cloth and the woman was stuck while cooking.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਪਤੀ ਧੋਤੀ ਬੰਨ੍ਰਹਦਿਆਂ ਫਸ ਗਿਆ ਅਤੇ ਇਸਤਰੀ ਰਸੋਈ ਤਿਆਰ ਕਰਦੀ ਫਸ ਗਈ।",
              "additional_information": {}
            }
          }
        }
