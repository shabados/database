        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "No one remained in his senses and lamentations were prevailing all over.(21)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸ਼ਹਿਰ ਵਿਚ ਬਹੁਤ ਚੀਖ ਚਹਾੜਾ ਪੈ ਗਿਆ ਅਤੇ ਕਿਸੇ ਵਿਚ ਸੁੱਧ ਬੁੱਧ ਨਾ ਰਹੀ ॥੨੧॥",
              "additional_information": {}
            }
          }
        }
      },
      {
