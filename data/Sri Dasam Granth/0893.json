        "source_line": null,
        "gurmukhi": "nOAw iqRX sovq PisXo; kCu n rhI suiD jIX ]22]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The husband sleeping with newly wed was stuck and no one remained rational.(22)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਨਵੀਂ ਵਿਆਹੀ ਇਸਤਰੀ (ਪਤੀ ਨਾਲ) ਸੁਤੀ ਹੋਈ ਫਸ ਗਈ ਅਤੇ (ਕਿਸੇ ਨੂੰ ਵੀ ਆਪਣੇ) ਜੀ ਦੀ ਹੋਸ਼ ਨਾ ਰਹੀ ॥੨੨॥",
              "additional_information": {}
            }
          }
        }
