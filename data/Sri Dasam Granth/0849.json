          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਬੈਠ ਕੇ ਤੇਲ ਵਿਚ ਭੋਜਨ ਤਿਆਰ ਕੀਤਾ",
              "additional_information": {}
            }
