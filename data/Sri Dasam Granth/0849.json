        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "And herself, putting the saucepan on fire, commenced cooking.(8)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਆਪ ਕੜਾਹੀ ਚੜ੍ਰਹਾ ਕੇ ਵੜੇ ਪਕਾ ਲਏ ॥੮॥",
              "additional_information": {}
            }
          }
        }
      }
