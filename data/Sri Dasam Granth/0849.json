      {
        "id": "68ZK",
        "source_page": 849,
        "source_line": null,
        "gurmukhi": "cor Awvq Aiq nwir inhwry ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦ ਚੋਰਾ ਨੂੰ ਆਉਾਂਦਿਆਂ ਇਸਤਰੀ ਨੇ ਚੰਗੀ ਤਰ੍ਰਹਾ ਵੇਖਿਆ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "JJBN",
        "source_page": 849,
        "source_line": null,
        "gurmukhi": "AYs Bwiq so bcn aucwry ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When she saw the thieves coming, she said,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾ ਇਸ ਤਰ੍ਰਹਾ ਕਹਿਣ ਲਗੀ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "GKNB",
        "source_page": 849,
        "source_line": null,
        "gurmukhi": "sunu qskr, mY nwir iqhwrI ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਚੋਰੋ! ਮੈਂ ਤੁਹਾਡੀ ਇਸਤਰੀ ਹਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "FX0X",
        "source_page": 849,
        "source_line": null,
        "gurmukhi": "ApnI jwn krhu rKvwrI ]4]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Listen, you, I am your woman, and considering as your own, protect me.(4)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਆਪਣੀ ਜਾਣ ਕੇ ਮੇਰੀ ਰਾਖੀ ਕਰੋ ॥੪॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
