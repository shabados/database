        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "From that a girl took birth, which she washed away in the River Sindh and, herself, departed to the heaven.(3)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਉਸ ਗਰਭ ਤੋਂ ਇਕ ਕੰਨਿਆ ਪੈਦਾ ਹੋਈ) ਉਸ ਨੂੰ (ਉਹ) ਸਿੰਧ ਨਦੀ ਵਿਚ ਸੁਟ ਕੇ ਸਵਰਗ ਲੋਕ ਨੂੰ ਚਲੀ ਗਈ ॥੩॥",
              "additional_information": {}
            }
          }
        }
