            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਸ ਤਰ੍ਹਾਂ ਕਹਿ ਕੇ ਬ੍ਰਾਹਮਣ ਇਸਤਰੀ ਸਮੇਤ ਸੜ ਗਿਆ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "V2X2",
        "source_page": 203,
        "source_line": null,
        "gurmukhi": "qj dyh kIXo surpur ibhwr ]35]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Saying this, the Brahmin was burnt to ashes alongwith his wife and went to heaven.35.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਦੇਹ ਨੂੰ ਤਿਆਗ ਕੇ ਸੁਵਰਗ ਵਲ ਚਲਾ ਗਿਆ ॥੩੫॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
