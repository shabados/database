          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦ ਉਸ ਨੂੰ ਗਰਭ ਰਹਿ ਗਿਆ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "RGK8",
        "source_page": 825,
