          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "A number of valiant ones were present there and Kaamkandla was dancing.(10)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਥੇ ਕਾਮਕੰਦਲਾ ਨਚ ਰਹੀ ਸੀ ॥੧੦॥",
              "additional_information": {}
            }
