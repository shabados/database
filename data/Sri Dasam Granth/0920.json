        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Raja, then, sent them away, and collected the leaves of lotus,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਵਿਛੌਣੇ ਵਾਗ ਵਿਛਵਾ ਦਿੱਤੇ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "2V2J",
