        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The fifth one is Anuraktata (attachment) and the sixth warrior is Samadhi (contemplaton)",
              "additional_information": {}
