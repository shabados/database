          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਪੰਜਵਾਂ 'ਅਨੁਰਕਤਤਾ' ਅਤੇ ਛੇਵਾਂ 'ਸਾਮਾਧ' (ਸਮਾਧੀ) ਨਿਡਰ ਯੋਧੇ ਹਨ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "F3UP",
        "source_page": 701,
        "source_line": null,
        "gurmukhi": "audm Aru aupkwr; Aimt AnjIq Anwkt ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Uddam (effort), Upakaar (benevolence) etc. are also unconquerable, invincible and ineffaceable",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "'ਉਦਮ' ਅਤੇ 'ਉਪਕਾਰ' (ਨਾਮ ਵਾਲੇ ਯੋਧੇ) ਨਾ ਮਿਟਣ ਵਾਲੇ, ਨਾ ਜਿਤੇ ਜਾ ਸਕਣ ਵਾਲੇ ਅਤੇ ਨਾ ਕਟੇ ਜਾ ਸਕਣ ਵਾਲੇ ਹਨ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "9AUG",
        "source_page": 701,
        "source_line": null,
        "gurmukhi": "ijh inriK sqRü qij Awsnin; ibmn icq Bwjq qvn ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Seeing them, the enemies forsake their seat and run away, while deviating from their position",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਨ੍ਹਾਂ ਨੂੰ ਵੇਖ ਕੇ ਵੈਰੀ ਆਸਣਾਂ ਨੂੰ ਛਡ ਕੇ ਬੁਝੇ ਹੋਏ ਮਨ ਨਾਲ ਉਥੋਂ ਭਜ ਜਾਂਦੇ ਹਨ।",
              "additional_information": {}
            }
          }
        }
