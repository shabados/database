        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The glory of these mighty warrior is spread all over the earth.33.260.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹਠੀ ਸੂਰਮੇ ਬਲ ਛਡ ਕੇ, ਯੁੱਧ ਵਿਚ ਹਾਰ ਮੰਨ ਕੇ ਅਣਮਿਥੇ ਠਾਠ ਭੁਲ ਕੇ ਚਲੇ ਜਾਂਦੇ ਹਨ ॥੨੬੦॥",
              "additional_information": {}
            }
          }
        }
      }
