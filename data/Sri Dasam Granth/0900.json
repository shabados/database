        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Everybody branded her as a liar without understanding the secret.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਇਸ ਭੇਦ ਨੂੰ ਹਿਰਦੇ ਵਿਚ ਜ਼ਰਾ ਜਿੰਨਾ ਵੀ ਨਹੀਂ ਸਮਝਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
