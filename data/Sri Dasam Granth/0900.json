        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "A year had gone by when she felt relieved of all her tribulations.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾਂ ਉਹ ਇਸਤਰੀ ਦੁਖਾਂ ਤੋਂ ਨਿਸਚਿੰਤ ਹੋ ਗਈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "GR5Z",
        "source_page": 900,
        "source_line": null,
        "gurmukhi": "vh iqh Gr ko kwmu clwvY ]",
        "pronunciation": null,
