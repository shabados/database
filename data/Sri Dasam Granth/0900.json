        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Rather she was plundered and banished from the country.(9)(1)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਤਦ ਹੀ ਦੇਸ ਨਿਕਾਲਾ ਦੇ ਦਿੱਤਾ ॥੯॥",
              "additional_information": {}
            }
          }
        }
      },
