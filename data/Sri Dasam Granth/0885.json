        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘If God sends a thief, we may keep him as our son.(3)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜੇ ਪਰਮਾਤਮਾ ਇਥੇ ਚੋਰ ਨੂੰ ਵੀ ਲੈ ਆਏ (ਤਾ ਉਸ ਨੂੰ ਵੀ ਅਸੀਂ) ਪੁੱਤਰ ਬਣਾ ਕੇ ਰਖ ਲਵਾਗੇ ॥੩॥",
              "additional_information": {}
            }
          }
        }
      }
