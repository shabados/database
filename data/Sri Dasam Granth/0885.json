          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਸ਼ਾਹਣੀ! ਸੁਣ, ਸਾਡੇ ਘਰ ਵਿਧਾਤਾ ਨੇ ਪੁੱਤਰ ਨਹੀਂ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
