        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then she saw Parduman and, modestly, bent her head in reverence.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਤਾ ਉਸ) ਇਸਤਰੀ ਨੇ ਸ਼ਰਮਾ ਕੇ ਆਪਣੀ ਧੌਣ ('ਨਾਰਿ') ਨੀਵੀਂ ਕਰ ਲਈ।",
              "additional_information": {}
