          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਚਿਤ੍ਰਕਲਾ ਨੇ ਉਥੇ (ਇਹ ਵੱਡਾ ਸਾਰਾ) ਮਹੱਲ ਉਸਾਰਿਆ (ਅਰਥਾਤ ਚਿਤਰਿਆ)।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "9HPH",
        "source_page": 1015,
        "source_line": null,
        "gurmukhi": "cOdh Bvn qhw iliK fwirs ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then Chitar Kala built a castle and all round she drew (drawings of) the fourteen regions.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਵਿਚ ਚੌਦਾ ਲੋਕ ਚਿਤਰ ਦਿੱਤੇ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "0Z05",
        "source_page": 1015,
        "source_line": null,
        "gurmukhi": "dyv dYq iqh TOr bnwey ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਵਿਚ ਦੇਵਤੇ, ਦੈਂਤ,",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "24U5",
        "source_page": 1015,
        "source_line": null,
        "gurmukhi": "gMDRb jC BujMg suhwey ]20]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She sketched the devils, the gods and Gandharabh Jachh.(20)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਗੰਧਰਬ, ਯਕਸ਼ ਅਤੇ ਨਾਗ ਬਣਾ ਕੇ ਸੁਸ਼ੋਭਿਤ ਕੀਤੇ ॥੨੦॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
