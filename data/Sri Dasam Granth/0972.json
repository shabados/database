        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "After an extreme meditation they had obtained the boon from Shiva that they could not be killed.(1)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਇਹ ਵਰਦਾਨ ਪ੍ਰਾਪਤ ਕੀਤਾ ਕਿ ਮਾਰਨ ਤੇ ਵੀ ਮਰ ਨਾ ਸਕਣ ॥੧॥",
              "additional_information": {}
            }
          }
        }
      }
