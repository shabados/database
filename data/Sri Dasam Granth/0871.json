          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਦੇਵਤੇ ਤੇ ਇੰਦਰ ਵੀ (ਉਸ) ਲੜਾਈ ਨੂੰ ਵੇਖਣ ਆਏ ॥੨੨॥",
              "additional_information": {}
            }
