        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Ram said to him “When you see the enemy without the trident of Shiva, then wage a war with him.”709.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦੋਂ ਹੀ ਉਸ ਨਾਲ ਘੋਰ ਯੁੱਧ ਮਚਾ ਦਿਓ ॥੭੦੯॥",
              "additional_information": {}
            }
          }
        }
      },
