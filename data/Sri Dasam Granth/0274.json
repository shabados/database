        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "One day Ram sent for the son of Sumitra and said to him :",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਉਸ ਨੂੰ ਮਥੁਰਾ ਦਾ ਰਾਜਾ ਬਣਾ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "5LBZ",
        "source_page": 274,
        "source_line": null,
