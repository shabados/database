        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When Hussain was killed, the warriors were in great fury.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦੋਂ ਹੁਸੈਨੀ ਮਾਰਿਆ ਗਿਆ, (ਤਾਂ ਪਠਾਣ) ਸੂਰਮਿਆਂ ਦੇ ਮਨ ਵਿਚ ਬਹੁਤ ਕ੍ਰੋਧ ਪੈਦਾ ਹੋਇਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "BW84",
        "source_page": 69,
        "source_line": null,
        "gurmukhi": "Bwij cly AvrY sbY; auiTXo ktocn jos ]53]",
