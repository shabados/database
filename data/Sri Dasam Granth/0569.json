        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "There was a loud sound and the heroic spirits began to dance tying small bells round the ankles",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮਾਨੋ ਯੁੱਧ ਵਿਚ ਫਿਰੰਗੀ ਨਚੇ ਹੋਣ ਅਤੇ ਘੁੰਘਰੂ ਵਜੇ ਹੋਣ।",
              "additional_information": {}
            }
          }
        }
