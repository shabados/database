          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਕਲਕੀ ਨੇ) ਤਿੰਨ ਸੌ ਹੱਥ ਉੱਚਾ ਖੰਡਾ ਧੂਹਿਆ ਹੋਇਆ ਹੈ।",
              "additional_information": {}
            }
          }
        }
