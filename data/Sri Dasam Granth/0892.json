        "gurmukhi": "bhu bkrI iqh dyhu crwvY ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਸ ਨੂੰ ਬਹੁਤ ਸਾਰੀਆਂ ਬਕਰੀਆਂ ਚਰਾਉਣ ਲਈ ਦਿਓ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "YT5S",
        "source_page": 892,
        "source_line": null,
        "gurmukhi": "idvs crwie rwiq Gr AwvY ]12]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Give him a few goats. He will take them out for grazing and will come back in the evening.’(12)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਦਿਨ ਨੂੰ ਚਰਾ ਕੇ ਰਾਤ ਨੂੰ ਘਰ ਆ ਜਾਵੇ ॥੧੨॥",
              "additional_information": {}
            }
          }
        }
      }
