          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਨੂੰ ਦੁਖੀ ਵੇਖ ਕੇ (ਉਨ੍ਹਾਂ ਦੇ) ਮਨ ਵਿਚ ਦਇਆ ਆ ਗਈ ॥੧੬॥",
              "additional_information": {}
            }
          }
        }
      }
