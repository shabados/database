          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਨਾਈ ਦੇ ਪੁੱਤਰ ਨੂੰ ਸ਼ਾਹ ਦੇ ਪੁੱਤਰ ਦੀ ਪਤਨੀ ਮਿਲਾ ਦਿੱਤੀ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "D94G",
        "source_page": 892,
        "source_line": null,
        "gurmukhi": "swhu puqR so XO kihXo; duAwry bYThu jwie ]11]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "And the Shah’s son was told to go and sit out side on the door step.(11)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਸ਼ਾਹ ਦੇ ਪੁੱਤਰ ਨੂੰ ਇਸ ਤਰ੍ਹਾਂ ਕਿਹਾ ਕਿ ਉਹ ਦਰਵਾਜ਼ੇ ਉਤੇ ਜਾ ਕੇ ਬੈਠੇ ॥੧੧॥",
              "additional_information": {}
            }
          }
        }
      }
