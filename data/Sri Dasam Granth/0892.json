        }
      },
      {
        "id": "802H",
        "source_page": 892,
        "source_line": null,
        "gurmukhi": "clq clq ssurwir kO gwv phUMcÎo Awie ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Walking and walking they reached the village of the in-laws.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤੁਰਦੇ ਤੁਰਦੇ ਉਹ ਸੌਹਰੇ ਪਿੰਡ ਆ ਪਹੁੰਚੇ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "R8QX",
        "source_page": 892,
        "source_line": null,
        "gurmukhi": "auqir n iqh suq swhu ko; hY pr ilXo crwie ]8]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "But he did not dismount and did not let him (the Shah’s son) to mount.(8)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਪਰ ਉਸ ਨੇ ਉਤਰ ਕੇ ਘੋੜੇ ਉਤੇ ਸ਼ਾਹ ਦੇ ਪੁੱਤਰ ਨੂੰ ਨਾ ਚੜ੍ਹਾਇਆ ॥੮॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "XFCE",
        "source_page": 892,
        "source_line": null,
        "gurmukhi": "swhu puqR iqh kih rihXo; lXo n qurY crwie ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The Shah’s son insisted but he did not let him ride the horse.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸ਼ਾਹ ਦਾ ਪੁੱਤਰ ਉਸ ਨੂੰ ਕਹਿੰਦਾ ਰਿਹਾ (ਪਰ ਉਸ ਨੇ) ਘੋੜੇ ਉਤੇ ਨਾ ਚੜ੍ਹਾਇਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "D59P",
        "source_page": 892,
        "source_line": null,
        "gurmukhi": "swhu puqR liK iqh DnI; skl imlq By Awie ]9]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "(People) came and met assuming the barber’s son to be the Shah’s son.(9)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਧਨੀ ਨੂੰ ਸ਼ਾਹ ਦਾ ਪੁੱਤਰ ਸਮਝ ਕੇ ਸਭ ਮਿਲਣ ਲਈ ਆਏ ॥੯॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
