    "lines": [
      {
        "id": "SX0A",
        "source_page": 468,
        "source_line": null,
        "gurmukhi": "svYXw ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Sirlekh",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "SWAYYA",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸਵੈਯਾ:",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "WNDL",
        "source_page": 468,
        "source_line": null,
        "gurmukhi": "hwir prY mnuhwir krY; khY ieau inRp, juD ibRQw n kreIXY ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Being tired, they began to persuade the king by saying, “O king! do not fight now uselessly",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਅਸੀਂ) ਹਾਰ ਗਏ ਹਾਂ, ਤਰਲੇ ਕਰਦੇ ਹਾਂ, ਹੇ ਰਾਜਨ! ਇਉਂ ਵਿਅਰਥ ਦਾ ਯੁੱਧ ਨਾ ਕਰੋ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "WW9J",
        "source_page": 468,
        "source_line": null,
        "gurmukhi": "fwir dY hwQn qy hQIAwrn; kop qjo suK swiq smeIXY ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "There is no warrior like you in all the three worlds and your praise has spread in all these worlds:",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੱਥਾਂ ਵਿਚ ਹਥਿਆਰ ਛਡ ਦਿਓ, ਕ੍ਰੋਧ ਨੂੰ ਤਿਆਗ ਦਿਓ ਅਤੇ ਸੁਖ ਸ਼ਾਂਤੀ ਵਿਚ ਸਮਾਈ ਕਰੋ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "KXBP",
        "source_page": 468,
        "source_line": null,
        "gurmukhi": "sUr n koaU BXo qumry sm; qyro pRqwp iqhUM puir geIXY ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Abandoning your weapons and ire, now be peaceful",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤੁਹਾਡੇ ਵਰਗਾ ਹੋਰ ਕੋਈ ਸੂਰਮਾ ਨਹੀਂ ਹੋਇਆ, ਤੇਰਾ ਹੀ ਯਸ਼ ਤਿੰਨਾਂ ਲੋਕਾਂ ਵਿਚ ਗਾਇਆ ਜਾਂਦਾ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "XDH3",
        "source_page": 468,
        "source_line": null,
        "gurmukhi": "Cwfiq hY hm ssqR sbY; su ibvwn cFo sur Dwim isDeIXY ]1714]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "We all forsake our weapons, go to heaven, mounting on the air-vehicle.”1714.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਸੀਂ ਸਾਰੇ ਵੀ (ਹੁਣ) ਸ਼ਸਤ੍ਰ ਛਡਦੇ ਹਾਂ, ਤੁਸੀਂ ਵਿਮਾਨ ਵਿਚ ਚੜ੍ਹੋ ਅਤੇ ਦੇਵ-ਲੋਕ ਵਲ ਚਾਲੇ ਪਾਓ ॥੧੭੧੪॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
  {
