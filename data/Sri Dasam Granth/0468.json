      },
      {
        "id": "88LV",
        "source_page": 468,
        "source_line": null,
        "gurmukhi": "sb dyvn Aru ik®sn; dIn hÍY jb kihE ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
