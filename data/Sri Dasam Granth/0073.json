        "source_page": 73,
        "source_line": null,
        "gurmukhi": "jy swDU srnI pry; iqn ky kvx ibcwr ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Those who have taken refuge with the saints, what can be said about them?",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਹੜੇ ਸੰਤਾ ਦੀ ਸ਼ਰਨ ਵਿਚ ਪਏ ਹਨ, ਉਨ੍ਰਹਾ ਦਾ ਕੀ ਵਿਚਾਰ ਕਹੀਏ?",
              "additional_information": {}
            }
          }
        }
