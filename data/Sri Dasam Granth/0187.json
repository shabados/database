        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“O Shiva  we have not recognized thee, you are supremely mighty and an ascetic.”",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤੁਸੀਂ ਵੱਡੇ ਤਪਸਵੀ ਤੇ ਬਲਵਾਨ ਹੋ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "9FFJ",
        "source_page": 187,
        "source_line": null,
        "gurmukhi": "sunq bcn Bey rudR ik®pwlw ]",
