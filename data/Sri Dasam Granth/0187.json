        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The god of love troubled greatly the god Shiva, who was without his consort, with which Shiva remained in great agony.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਸ ਕਰ ਕੇ ਸ਼ਿਵ ਨੇ ਬਹੁਤ ਦੁਖ ਪਾਇਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
