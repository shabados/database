          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਪਰ ਅੰਤ ਨੂੰ) ਬਹੁਤ ਕ੍ਰੋਧ ਕਰ ਕੇ ਸ਼ਿਵ ਨੇ ਕਾਮ ਨੂੰ ਸਾੜ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
