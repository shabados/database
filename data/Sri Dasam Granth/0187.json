        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "End of the description of the Killing of Daksha, the greatness of rudra and the Killing of Gauri (Parvati) in the Rudra incarnation.11.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਥੇ ਸ੍ਰੀ ਬਚਿਤ੍ਰ ਨਾਟਕ ਗ੍ਰੰਥ ਦੇ 'ਰੁਦ੍ਰ ਪ੍ਰਬੰਧ ਦਕਸ਼-ਬਧ ਰੁਦ੍ਰ ਮਹਾਤਮ ਅਤੇ ਗੌਰੀ ਬੱਧ' ਯਾਰ੍ਹਵਾਂ ਅਧਿਆਇ ਸਮਾਪਤ, ਸਭ ਸ਼ੁਭ ਹੈ ॥੧੧॥",
              "additional_information": {}
            }
          }
        }
      }
