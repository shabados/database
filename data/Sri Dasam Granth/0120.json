          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਦੁਰਗਾ ਦੇਵੀ! ਇਸ ਕਰਕੇ (ਮੈਂ ਹੁਣ) ਤੇਰੀ ਸ਼ਰਨ ਵਿਚ ਆਇਆ ਹਾ ॥੪॥",
              "additional_information": {}
            }
          }
        }
      }
