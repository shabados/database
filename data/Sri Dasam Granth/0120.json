        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“They have played musical instruments in their rejoicings in Amaravati, the city of gods.\"",
              "additional_information": {}
            }
