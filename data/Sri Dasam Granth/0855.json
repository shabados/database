        "pronunciation_information": null,
        "type": "Pankti",
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "To kill her husband she struck fifty times in the dark.(l4)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਆਪਣੇ ਪਤੀ ਨੂੰ ਮਾਰਨ ਲਈ ਪੰਜਾਹ ਕੁ ਵਾਰ ਕੀਤੇ ॥੧੪॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "BHZL",
        "source_page": 855,
        "source_line": null,
        "gurmukhi": "inriK cmk qrvwr kI; durXw mihK qr jwie ]",
