        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Whatever she awarded, every body accepted and none demonstrated any defiance.(25)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ (ਉਧਰ) ਸਾਰਿਆਂ ਦੇਵਤਿਆਂ ਅਤੇ ਦੈਂਤਾ ਦੇ ਚਿਤ ਮੋਹੇ ਹੋਏ ਸਨ ॥੨੫॥",
              "additional_information": {}
            }
