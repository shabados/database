          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਧਰ (ਉਹ) ਆਪ ਸੁੰਦਰ ਬਸਤ੍ਰ ਪਾ ਕੇ ਸ਼ੋਭ ਰਿਹਾ ਸੀ",
              "additional_information": {}
            }
          }
        }
