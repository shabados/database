        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Elephant, parrots, Moon, lions and the Cupid cast off their egos.",
              "additional_information": {}
