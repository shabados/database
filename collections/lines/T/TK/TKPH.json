      "line": 1
    },
    {
      "asset": "sbms",
      "type": "translation",
      "language": "en",
      "data": "Thou art such a merciful and compassionate Lord,"
    },
    {
      "asset": "sbms",
      "type": "translation",
      "language": "pa",
      "data": "ਤੂੰ ਐਸਾ ਮਿਹਰਬਾਨ ਤੇ ਮਾਇਆਵਾਨ ਮਾਲਕ ਹੈ,"
    },
    {
      "asset": "dssk",
      "type": "translation",
      "language": "en",
